   "source": [
    "But this is relatively easy to manage, we'll just keep information about the birthYear (it exists as a relation in DBPedia, but it's not specified for many actors, so I took the birthDate instead), and we'll concatenate values for different nationalities into one column : "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 60,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{'year': 1977, 'nationalities': 'German, Irish', 'height': 1.524}\n"
     ]
    }
   ],
   "source": [
    "actor_data = {}\n",
    "for name in df_actors_raw['name'].unique():\n",
    "    nationalities = set()\n",
    "    year, height = 0, 0\n",
    "    for i, row in df_actors_raw[df_actors_raw['name'] == name].iterrows():\n",
    "        try: \n",
    "            if pd.isnull(row['birthDate']) : year = np.NaN\n",
    "            else: \n",
    "                try: year = int(row['birthDate'][:4])\n",
    "                except: year = int(row['birthDate'][4:])\n",
    "            height = row['height']\n",
    "            nationalities.add('' if pd.isnull(row['nationality']) else row['nationality'].split('/')[-1].split('_')[0]) #we take the first word from the nationality URI : British_People => British\n",
    "        except:\n",
    "            print(row)\n",
    "            print(row['nationality'])\n",
    "    actor_data[name] = {'year': year, 'nationalities': ', '.join(nationalities), 'height': height}\n",
    "    \n",
    "print(actor_data['Michael Fassbender'])"
   ]
