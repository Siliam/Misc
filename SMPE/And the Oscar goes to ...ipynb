  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "select ?actor ?name ?birthDate ?height ?nationality\n",
    "where\n",
    "{\n",
    "   ?actor <http://purl.org/linguistics/gold/hypernym> ?v;\n",
    "           foaf:name ?name.\n",
    "   OPTIONAL { ?actor dbo:height ?height. }\n",
    "   OPTIONAL { ?actor dbo:birthDate ?birthDate. ?birthDate rdf:type xsd:date}\n",
    "   OPTIONAL { ?actor dbo:birthDate ?birthDate. }\n",
    "   OPTIONAL { ?actor dbo:nationality ?nationality. } \n",
    "\n",
    "   FILTER (?name IN (\"Richard Barthelmess\"@en, \"Emil Jannings\"@en, \"Louise Dresser\"@en, \"Janet Gaynor\"@en, \"Gloria Swanson\"@en,                      \"George Bancroft\"@en, \"Warner Baxter\"@en, \"Chester Morris\"@en, \"Paul Muni\"@en, \"Lewis Stone\"@en ..))\n",
    "\n",
    "   FILTER (?v in (dbr:Actor, dbr:Actress))\n",
    "}"
   ]
  },
